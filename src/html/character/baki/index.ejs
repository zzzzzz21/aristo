page = '';
%><!DOCTYPE html>
<html lang="ja">
<% include ../../include/_head %>
<body class="<% if (directoryLv2!=='') { %><%= directory %> <%= directory %>-<%= directoryLv2 %><% } else if (directory!=='') { %> <%= directory %><% } else {%>home<% } %>" id="top">
<div class="page" id="data-page">
<% include ../../include/_header %>

<% include ../../include/_top-carousel %>
	
	<div class="main contents">
		<section class="contents__inner">
			<h1 class="page-title"><img src="<%= path %>images/character/page-title.png" alt="<%= meta.title %>"></h1>
			<div class="page-body">
<% include ../../include/_character-navi %>

				<div class="character-article">
					<div class="character-article__body">
						<div class="character-article__wrapper">
							<div class="character-article__title">
								<p class="character-article__shoulder"><img src="<%= path %>images/character/baki/shoulder.png" alt="地下闘技場チャンピオン"></p>
