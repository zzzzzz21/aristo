<%
meta = { title:'キャラクター：スペック', desc:'' };
path = '../../';
directory = 'character';
directoryLv2 = 'spec';
page = '';
%><!DOCTYPE html>
<html lang="ja">
<% include ../../include/_head %>
<body class="<% if (directoryLv2!=='') { %><%= directory %> <%= directory %>-<%= directoryLv2 %><% } else if (directory!=='') { %> <%= directory %><% } else {%>home<% } %>" id="top">
<div class="page" id="data-page">
<% include ../../include/_header %>

<% include ../../include/_top-carousel %>
	
	<div class="main contents">
		<section class="contents__inner">
			<h1 class="page-title"><img src="<%= path %>images/character/page-title.png" alt="<%= meta.title %>"></h1>
			<div class="page-body">
<% include ../../include/_character-navi %>

				<div class="character-article">
					<div class="character-article__body">
						<div class="character-article__wrapper">
							<div class="character-article__title">
								<p class="character-article__shoulder"><img src="<%= path %>images/character/spec/shoulder.png" alt="最凶死刑囚"></p>
								<h2 class="character-article__name"><img src="<%= path %>images/character/spec/name.png" alt="スペック"></h2>
								<p class="character-article__cv"><img src="<%= path %>images/character/spec/cv.png" alt="CV茶風林"></p>
							</div>
							<p class="character-article__image"><img src="<%= path %>images/character/spec/character_main.png" alt="スペック：全身"></p>
						</div>
					</div>
					<div class="character-article__sub">
						<div class="character-article__imageSub"><img src="<%= path %>images/character/spec/character_sub.png" alt="スペック：表情"></div>
						<div class="character-article__description">
							<p>アメリカの海底刑務所から脱獄。<br>超人的な心肺能力を誇り5分間の無呼吸運動が可能。</p>
						</div>
					</div>
				</div><!-- /[.character-article] -->
			</div>
		</section>
	</div>


<% include ../../include/_footer %>
</div><!-- /[.page] -->

<% include ../../include/_loading %>

<% include ../../include/_body_last %>
</body>
</html>
